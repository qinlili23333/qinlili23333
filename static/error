<body>
    <img id="erroremoji"
        src="data:image/webp;base64,">
    <H3>加载失败</H3>
    <p id="link"></p>
    <button id="back">
        <img style="height:20px;"
            src="data:image/svg+xml;base64,">
        返回</button>
    <br>
    <button id="refresh">
        <img style="height:20px;"
            src="data:image/svg+xml;base64,">
        重试</button>
    <br>
    <button id="setting">
        <img style="height:20px;"
            src="data:image/svg+xml;base64,">
        设置网络</button>
    <br>
    <div style="padding-top:20px;display: flex;flex-direction: row;justify-content: center;">
        <img id="icon" style="height:20px;" src="/img/loading.svg">
        <p id="detect">正在检测...</p>
    </div>
    <div id="trace"></div>
    <script>
        document.getElementById("back").addEventListener("click", () => {
            history.go(-1);
        });
        document.getElementById("refresh").addEventListener("click", () => {
            location.reload();
        });
        document.getElementById("setting").addEventListener("click", () => {
            switch (navigator.userAgentData.platform) {
                case "Windows": {
                    window.open("ms-settings:network");
                    break;
                };
                default: {
                    alert("暂不支持该平台");
                }
            }
        });
        document.getElementById("link").innerText = "请求地址：" + location.pathname;
        if (!(window.self === window.top)) {
            document.getElementById("back").style.display = "none";
        }
        const detect = async () => {
            let status = document.getElementById("detect");
            let trace = document.getElementById("trace");
            if (!navigator.onLine) {
                status.innerText = "设备未连接网络"
                document.getElementById("icon").style.display = "none";
                return;
            }
            await fetch("https://www.cloudflare.com/?ForceNoCache=1", { method: "HEAD", mode: "no-cors" }).then(() => {
                let info = document.createElement("p");
                document.getElementById("trace").appendChild(info);
                info.innerText = "Cloudflare连接成功"
                info.style.color = "green";
            }).catch(() => {
                let info = document.createElement("p");
                document.getElementById("trace").appendChild(info);
                info.innerText = "Cloudflare连接失败"
                info.style.color = "red";
                document.getElementById("icon").style.display = "none";
                status.innerText = "请检查和Cloudflare的连接，可能是内网占用1.1.1.1导致";
                return false;
            });
            await fetch("https://qinlili.bid/?ForceNoCache=1", { method: "HEAD", mode: "no-cors" }).then(() => {
                let info = document.createElement("p");
                document.getElementById("trace").appendChild(info);
                info.innerText = "琴梨梨的小站连接成功"
                info.style.color = "green";
            }).catch(() => {
                let info = document.createElement("p");
                document.getElementById("trace").appendChild(info);
                info.innerText = "琴梨梨的小站连接失败"
                info.style.color = "red";
                document.getElementById("icon").style.display = "none";
                status.innerText = "请检查是否拦截琴梨梨的域名，你的网络可能存在DNS污染或SNI阻断";
                return false;
            });
            await fetch(location.origin + "/?ForceNoCache=1", { method: "HEAD", mode: "no-cors" }).then(() => {
                let info = document.createElement("p");
                document.getElementById("trace").appendChild(info);
                info.innerText = "545在线连接成功"
                info.style.color = "green";
                document.getElementById("icon").style.display = "none";
                status.innerText = "似乎不是网络问题，请截图反馈";
                return false;
            }).catch(() => {
                let info = document.createElement("p");
                document.getElementById("trace").appendChild(info);
                info.innerText = "545在线连接失败"
                info.style.color = "red";
                document.getElementById("icon").style.display = "none";
                status.innerText = "试试更换一个节点或许有所帮助";
                return false;
            });
        }
        detect();
    </script>
